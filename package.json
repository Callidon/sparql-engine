  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Callidon/sparql-engine/issues"
  },
  "contributors": [
    "Corentin Marionneau <corentin.marionneau@gmail.com> (https://github.com/Slaanaroth)",
    "Merlin Barzilai (https://github.com/Rintarou)"
  ],
  "homepage": "https://github.com/Callidon/sparql-engine#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "docma": "^2.1.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "asynciterator": "^2.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "sparqljs": "^2.0.3",
    "n3": "^0.11.3",
    "uuid": "^3.3.2"
  },
  "standard": {
    "env": [
      "node",
      "mocha",
      "chai"
    ]
